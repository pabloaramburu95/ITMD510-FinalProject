<StackPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="700.0"
	prefWidth="657.0" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controllers.PatientMainViewController">
	<children>
		<VBox prefHeight="578.0" prefWidth="572.0" 
			alignment="center" spacing="20">
			<HBox spacing="40">
				<children>
					<Label fx:id="userLabel" prefHeight="27.0" prefWidth="190.0"
						text="Welcome back username!" />
				</children>
				<children>
					<Label fx:id="doctorLabel" prefHeight="27.0"
						prefWidth="200.0" text="Your doctor is doctor" />
				</children>
			</HBox>
			<HBox prefHeight="55.0" prefWidth="636.0" spacing="15.0">
				<GridPane alignment="CENTER" prefHeight="120.0"
					prefWidth="684.0" vgap="10.0" hgap="10">
					<children>
						<!-- <TextField fx:id="searchBar" prefHeight="24.0" prefWidth="251.0" 
							promptText="Search..." /> -->
						<Button fx:id="measuresButton" mnemonicParsing="false"
							text="My Measures" GridPane.columnIndex="0" GridPane.rowIndex="0" />
						<Button fx:id="treatmentButton" mnemonicParsing="false"
							text="My Treatment" GridPane.columnIndex="1"
							GridPane.rowIndex="0" />
							<Button fx:id="postsButton" mnemonicParsing="false"
							text="View posts" GridPane.columnIndex="2"
							GridPane.rowIndex="0" />
						<Button fx:id="logoutButton" mnemonicParsing="false"
							text="Log out" GridPane.columnIndex="3" GridPane.rowIndex="0" />
					</children>
					<padding>
						<Insets bottom="15.0" left="10.0" right="10.0" top="20.0" />
					</padding>
				</GridPane>
			</HBox>
			<ImageView fx:id="patientImage" fitHeight="400.0"

				pickOnBounds="true" preserveRatio="true">
			</ImageView>
		</VBox>
	</children>
	<stylesheets>
		<URL value="@/application/application.css" />
	</stylesheets>
</StackPane>